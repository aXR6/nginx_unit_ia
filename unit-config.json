{
    "listeners": {
        "*:8080": {
            "pass": "routes"
        }
    },
    "applications": {
        "dummy": {
            "type": "python",
            "path": "/www/app",
            "module": "wsgi"
        }
    },
    "routes": [
        {
            "match": { "uri": "/hello" },
            "action": { "proxy": "http://hello:8000" }
        },
        {
            "action": { "pass": "applications/dummy" }
        }
    ]
}
